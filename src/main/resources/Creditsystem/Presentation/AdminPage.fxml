            <Separator layoutX="14.0" layoutY="277.0" prefWidth="207.0" style="-fx-background-color: white;" />
         </children>
      </AnchorPane>
      <Button fx:id="btnBack" layoutX="925.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" style="-fx-background-color: #d21e1e;" text="Tilbage" textFill="WHITE">
