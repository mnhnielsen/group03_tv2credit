                  <Image url="@Images/TV_2_Hvid_RGB.png" />
               </image>
            </ImageView>
            <Label layoutX="80.0" layoutY="13.0" text="Admin side" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Label>
            <Label layoutX="51.0" layoutY="248.0" text="Opret ny bruger" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <PasswordField fx:id="pwrdPassword" layoutX="14.0" layoutY="332.0" prefHeight="25.0" prefWidth="207.0" promptText="password" style="-fx-border-color: red;" />
            <PasswordField fx:id="pswrdReEnterPassword" layoutX="14.0" layoutY="372.0" prefHeight="30.0" prefWidth="207.0" promptText="genindtast password" style="-fx-border-color: red;" />
